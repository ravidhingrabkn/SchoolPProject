@model IEnumerable<SchoolProject.Models.Teacher>


@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<p>
    List showing all Teachers in our Database
</p>
<div id="containerlist">
    <h2>List of Teachers</h2>

    @foreach (var teacher in Model)
    {
<div class="listitem row">
    <div class="col-lg-12"><a href="/Teacher/Show/@teacher.TeacherId">@teacher.TeacherFname @teacher.TeacherLname</a></div>
</div>
    }
</div>